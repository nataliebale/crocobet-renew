{
  "name": "crocobet-new",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start-web": "nx serve crocobet-web --host dev.crocobet.com --port 3000 --disable-host-check --ssl",
    "start-mobile": "nx serve crocobet-mobile --host mobile.crocobet.com --port 80 --disable-host-check",
    "start-demo": "nx serve components-demo",
    "build-web": "nx run crocobet-web:build:test",
    "build-mobile": "nx build crocobet-mobile",
    "build-stats": "nx build --statsJson",
    "build-effected:develop": "nx affected:build --base=develop",
    "test": "nx test",
    "prepare": "husky install",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/apps/crocobet-web/browser/stats.json",
    "compodoc": "nx affected --target=doc",
    "dev-web:ssr": "ng run crocobet-web:serve-ssr",
    "serve-web:ssr": "node dist/apps/crocobet-web/server/main.js",
    "build-web:ssr": "ng build && ng run crocobet-web:server",
    "prerender": "ng run crocobet-web:prerender"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix",
    "*.{html,json}": "prettier --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.14",
    "@angular/cdk": "~13.1.1",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/forms": "^12.2.0",
    "@angular/google-maps": "^13.1.2",
    "@angular/material": "~13.1.1",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/platform-server": "^12.2.0",
    "@angular/router": "^12.2.0",
    "@ng-select/ng-select": "^8.1.1",
    "@ngneat/until-destroy": "^8.1.4",
    "@ngrx/effects": "^12.5.1",
    "@ngrx/store": "^12.5.0",
    "@ngrx/store-devtools": "^12.5.1",
    "@nguniversal/express-engine": "^12.1.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nrwl/angular": "12.10.0",
    "bootstrap": "^5.1.3",
    "domino": "^2.1.6",
    "express": "^4.15.2",
    "ngx-clipboard": "^14.0.2",
    "ngx-infinite-scroll": "^10.0.1",
    "ngx-mask": "^13.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.4.0",
    "swiper": "^7.0.0",
    "tslib": "^2.0.0",
    "xmldom": "^0.6.0",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/language-service": "^12.2.0",
    "@compodoc/compodoc": "^1.1.15",
    "@ngneat/spectator": "^8.1.0",
    "@nguniversal/builders": "^12.1.1",
    "@nrwl/cli": "12.10.0",
    "@nrwl/devkit": "12.10.0",
    "@nrwl/eslint-plugin-nx": "12.10.0",
    "@nrwl/jest": "12.10.0",
    "@nrwl/linter": "12.10.0",
    "@nrwl/tao": "12.10.0",
    "@nrwl/workspace": "12.10.0",
    "@types/express": "^4.17.0",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-ngrx": "^1.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-rxjs": "^4.0.1",
    "husky": "^7.0.2",
    "jest": "27.2.3",
    "jest-preset-angular": "10.0.1",
    "lint-staged": "^11.2.3",
    "ng-packagr": "^12.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
